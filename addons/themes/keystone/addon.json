{
    "key": "keystone",
    "name": "Keystone",
    "description": "A responsive Vanilla theme with customization options.",
    "version": "2.0.1",
    "authors": [
        {
            "name": "Isis Graziatto",
            "email": "isis.g@vanillaforums.com",
            "homepage": "https://vanillaforums.com"
        },
        {
            "name": "Adam (charrondev) Charron",
            "email": "adam.c@vanillaforums.com",
            "homepage": "http://vanillaforums.com"
        }
    ],
    "isResponsive": true,
    "license": "GPL-2.0-only",
    "sites": [],
    "priority": 1000,
    "type": "theme",
    "build": {
        "process": "v1"
    },
    "options": {
        "Styles": {
            "Default": "%s_default",
            "Classic": "%s_classic",
            "Dark": "%s_dark",
            "Cerulean": "%s_cerulean",
            "Coral": "%s_coral",
            "Dusk": "%s_dusk"
        }
    },
    "assets": {
        "variables_default": {
            "type": "json",
            "file": "variables_default.json"
        },
        "variables_classic": {
            "type": "json",
            "file": "variables_classic.json"
        },
        "variables_coral": {
            "type": "json",
            "file": "variables_coral.json"
        },
        "variables_dusk": {
            "type": "json",
            "file": "variables_dusk.json"
        },
        "variables_dark": {
            "type": "json",
            "file": "variables_dark.json"
        },
        "variables_cerulean": {
            "type": "json",
            "file": "variables_cerulean.json"
        }
    },
    "Features": {
        "LegacyDataDrivenTheme": true
    }
}
