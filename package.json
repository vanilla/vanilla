{
  "name": "vanilla",
  "description": "Vanilla is a powerfully simple discussion forum you can easily customize to make as unique as your community.",
  "license": "GPL-2.0-only",
  "private": true,
  "prettier": "@vanilla/prettier-config",
  "workspaces": {
    "packages": [
      "packages/*",
      "applications/dashboard",
      "applications/vanilla",
      "plugins/*",
      "addons/themes/*"
    ]
  },
  "engines": {
    "yarn": "*",
    "node": ">=20.0.0"
  },
  "scripts": {
    "pre-push": "yarn run build && yarn run test && yarn run check-types",
    "build": "yarn run build:assets && yarn run build:variable-docs",
    "build:assets": "./cli/bin/vnla build",
    "build:dev": "./cli/bin/vnla build --dev",
    "build:analyze": "./cli/bin/vnla build --analyze",
    "build:variable-docs": "node -r esbuild-register ./build/scripts/variables/buildVariableDocs.ts",
    "build:legacyDashboard": "node -r esbuild-register ./build/vite.buildLegacyDashboard.ts",
    "coverage:merge": "node -r esbuild-register build/scripts/mergeReports.ts",
    "update:tsconfig": "node ./packages/updateTsconfig.js",
    "update:codeSettings": "node -r esbuild-register build/scripts/generateVscodeSettings.ts",
    "test": "yarn run test:unit:withCoverage && yarn run test:stories:withCoverage",
    "test:stories:withCoverage": "node --preserve-symlinks --preserve-symlinks-main node_modules/.bin/jest --watchAll=false --coverage --coverageDirectory=coverage/storyshots --roots=\"<rootDir>\" --testMatch=\"<rootDir>/storyshots/index.js\"",
    "test:unit": "node ./node_modules/.bin/vitest --reporter=basic --ui --disable-console-intercept",
    "test:unit:withCoverage": "RUN_STORYBOOK_TESTS=true node ./node_modules/.bin/vitest --coverage --watch=false",
    "check-types": "tsc -p tsconfig.json --noemit",
    "check-types:watch": "tsc -p tsconfig.json --noemit --watch",
    "lint": "yarn jest --config ./jest.eslint.config.mjs",
    "storybook": "storybook dev -p 9001 -c build/.storybook",
    "build-storybook": "NODE_ENV=\"production\" storybook build -c build/.storybook -o build/.out",
    "test-storybook": "test-storybook --url http://localhost:9001 -c build/.storybook"
  },
  "madge": {
    "tsConfig": "./tsconfig.json",
    "extensions": ".ts,.tsx",
    "excludeRegExp": [
      "dist"
    ],
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      }
    }
  },
