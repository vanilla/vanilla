{
    "key": "keystone",
    "name": "Keystone",
    "description": "A responsive Vanilla theme with customization options.",
    "version": "1.0.0",
    "hidden": false,
    "authors": [
        {
            "name": "Isis Graziatto",
            "email": "isis.g@vanillaforums.com",
            "homepage": "https://vanillaforums.com"
        },
        {
            "name": "Adam (charrondev) Charron",
            "email": "adam.c@vanillaforums.com",
            "homepage": "http://vanillaforums.com"
        }
    ],
    "isResponsive": true,
    "license": "GPL-2.0-only",
    "sites": [],
    "priority": 1000,
    "type": "theme",
    "build": {
        "process": "v1"
    },
    "options": {
        "Styles": {
            "Default": "%s_default",
            "Classic": "%s_classic",
            "Dark": "%s_dark",
            "Cerulean": "%s_cerulean",
            "Coral": "%s_coral",
            "Dusk": "%s_dusk"
        }
    },
    "assets": {
        "header": {
            "type": "html",
            "file": "header.html"
        },
        "footer": {
            "type": "html",
            "file": "footer.html"
        },
        "variables": {
            "type": "json",
            "file": "variables.json"
        },
        "fonts": {
            "type": "data",
            "file": "fonts.json"
        },
        "scripts": {
            "type": "data",
            "file": "scripts.json"
        },
        "styles": {
            "type": "css",
            "file": "styles.css"
        },
        "javascript": {
            "type": "js",
            "file": "javascript.js"
        }
    }
}
